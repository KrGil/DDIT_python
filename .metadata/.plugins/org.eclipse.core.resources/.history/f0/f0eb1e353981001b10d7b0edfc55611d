def add(a, b):
    return a+b

sum = add(1,5)
print(sum)

#minus, mul, div, mymod

def minus(a, b):
    return a-b

def mul(a,b):
    return a*b

def div(a,b):
    return a/b

def mymod(a, b):
    return a%b

sum = minus(2, 3)
print(sum)
sum = mul(2, 3)
print(sum)
sum = div(2, 3)
print(sum)
sum = mymod(2, 3)
print(sum)



